<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="TabItem">
        <attr name="tab_icon" format="reference" />
        <attr name="tab_text" format="string" />
        <attr name="tab_text_color" format="color" />
        <attr name="unselected_tab_text_color" format="color" />
        <attr name="unselected_tab_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="BottomNavigation">
        <attr name="mode" format="integer">
            <enum name="tablet" value="1" />
            <enum name="phone" value="0" />
        </attr>
    </declare-styleable>
    <declare-styleable name="ScrimInsetsView">
        <attr name="attribute" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="CropImageView">
        <attr name="guidelines">
            <enum name="on" value="2" />
            <enum name="onTouch" value="1" />
            <enum name="off" value="0" />
        </attr>
        <attr name="fixAspectRatio" format="boolean" />
        <attr name="aspectRatioX" format="integer" />
        <attr name="aspectRatioY" format="integer" />
        <attr name="imageResource" format="reference" />
    </declare-styleable>
    <declare-styleable name="RoundedImageView">
        <attr name="corner_radius" format="dimension" />
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="round_background" format="boolean" />
        <attr name="is_oval" format="boolean" />
        <attr name="android:scaleType" />
    </declare-styleable>
    <declare-styleable name="app">
        <attr name="bottomDragVisibleHeight" format="dimension" />
        <attr name="bototmExtraIndicatorHeight" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LoadListEbookRecyclerView">
        <attr name="demo_layout" format="reference" />
        <attr name="demo_child_count" format="integer" />
        <attr name="demo_layout_manager_type" format="enum">
            <enum name="linear_vertical" value="0" />
            <enum name="linear_horizontal" value="1" />
            <enum name="grid" value="2" />
        </attr>
        <attr name="demo_grid_child_count" format="integer" />
    </declare-styleable>
    <declare-styleable name="NavigationTabBar">
        <attr name="ntb_titled" format="boolean" />
        <attr name="ntb_badged" format="boolean" />
        <attr name="ntb_scaled" format="boolean" />
        <attr name="ntb_tinted" format="boolean" />
        <attr name="ntb_swiped" format="boolean" />
        <attr name="ntb_badge_use_typeface" format="boolean" />

        <attr name="ntb_title_size" format="dimension" />
        <attr name="ntb_title_mode" format="enum">
            <enum name="all" value="0" />
            <enum name="active" value="1" />
        </attr>

        <attr name="ntb_badge_size" format="dimension" />
        <attr name="ntb_badge_position" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="ntb_badge_gravity" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
        </attr>
        <attr name="ntb_badge_bg_color" format="color" />
        <attr name="ntb_badge_title_color" format="color" />

        <attr name="ntb_typeface" format="string" />
        <attr name="ntb_corners_radius" format="dimension" />
        <attr name="ntb_icon_size_fraction" format="float" />
        <attr name="ntb_animation_duration" format="integer" />

        <attr name="ntb_inactive_color" format="color" />
        <attr name="ntb_active_color" format="color" />
        <attr name="ntb_bg_color" format="color" />

        <attr name="ntb_preview_colors" format="reference" />
    </declare-styleable>
    <declare-styleable name="MaterialSpinner" tools:ignore="ResourceName">
        <attr name="ms_arrow_tint" format="color" />
        <attr name="ms_hide_arrow" format="boolean" />
        <attr name="ms_background_color" format="color" />
        <attr name="ms_text_color" format="color" />
        <attr name="ms_dropdown_max_height" format="dimension" />
        <attr name="ms_dropdown_height" format="dimension">
            <enum name="fill_parent" value="-1" />
            <enum name="match_parent" value="-1" />
            <enum name="wrap_content" value="-2" />
        </attr>
    </declare-styleable>
    <declare-styleable name="Msv">
        <attr name="msvSearchBackground" format="reference" />
        <attr name="msvHint" format="string" />
        <attr name="msvTextColor" format="color" />
        <attr name="msvTextColorHint" format="color" />
        <attr name="msvIsContainEnabled" format="boolean" />
        <attr name="msvHideShadow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleWithArcProgress">
        <attr name="radius1" format="dimension"/>
        <attr name="circleColor" format="color"/>
        <attr name="arcColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="ColorfulProgress">
        <attr name="circleColor1" format="color"/>
        <attr name="circleColor2" format="color"/>
        <attr name="circleColor3" format="color"/>
        <attr name="circleColor4" format="color"/>
    </declare-styleable>

    <declare-styleable name="DoubleArcProgress">
        <attr name="insideArcRadius" format="dimension"/>
        <attr name="outsideArcRadius" format="dimension"/>
        <attr name="insideArcColor" format="color"/>
        <attr name="outsideArcColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="SimpleArcProgress">
        <attr name="arcRadius" format="dimension"/>
        <attr name="colorofArc" format="color"/>
    </declare-styleable>

    <declare-styleable name="StaticCircleProgress">
        <attr name="innerCircleRadius" format="dimension"/>
        <attr name="innerCircleColor" format="color"/>
        <attr name="movingArcRadius" format="dimension"/>
        <attr name="movinArcColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="starbutton">
        <attr name="starbutton_iconSize" format="dimension|reference" />
        <attr name="starbutton_activeImage" format="reference" />
        <attr name="starbutton_inActiveImage" format="reference" />
        <attr name="starbutton_primaryColor" format="reference" />
        <attr name="starbutton_secondaryColor" format="reference" />
        <attr name="starbutton_pressOnTouch" format="boolean" />
        <attr name="starbutton_animationSpeed" format="float" />
        <attr name="starbutton_activeImageTint" format="reference" />
        <attr name="starbutton_inActiveImageTint" format="reference" />
    </declare-styleable>

    <declare-styleable name="ExpandableTextView">
        <attr name="trimLength" format="integer"/>
    </declare-styleable>
    <declare-styleable name="ProgressLayout">
        <attr name="loadingProgressBarRadius" format="dimension" />
        <attr name="loadingProgressBarSpinWidth" format="dimension" />
        <attr name="loadingProgressBarColor" format="color" />
        <attr name="loadingBackgroundColor" format="color" />

        <attr name="emptyImageWidth" format="dimension" />
        <attr name="emptyImageHeight" format="dimension" />
        <attr name="emptyContentTextSize" format="dimension" />
        <attr name="emptyContentTextColor" format="color" />
        <attr name="emptyBackgroundColor" format="color" />

        <attr name="errorImageWidth" format="dimension" />
        <attr name="errorImageHeight" format="dimension" />
        <attr name="errorContentTextSize" format="dimension" />
        <attr name="errorContentTextColor" format="color" />
        <attr name="errorButtonTextSize" format="dimension" />
        <attr name="errorButtonTextColor" format="color" />
        <attr name="errorBackgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="ProgressWheel">
        <attr name="matProg_progressIndeterminate" format="boolean" />
        <attr name="matProg_barColor" format="color" />
        <attr name="matProg_rimColor" format="color" />
        <attr name="matProg_rimWidth" format="dimension" />
        <attr name="matProg_spinSpeed" format="float" />
        <attr name="matProg_barSpinCycleTime" format="integer" />
        <attr name="matProg_circleRadius" format="dimension" />
        <attr name="matProg_fillRadius" format="boolean" />
        <attr name="matProg_barWidth" format="dimension" />
        <attr name="matProg_linearProgress" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>
</resources>